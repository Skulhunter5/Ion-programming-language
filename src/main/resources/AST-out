<AST-BLOCK children=[
	<AST-EXPRESSION-ASSIGNMENT
		identifier='test'
		value=
			<AST-EXPRESSION-INTEGER
				value='0'
			>
	>,
	<AST-STATEMENT-WHILE
		condition=
			<AST-EXPRESSION-COMPARISON
				a=
					<AST-EXPRESSION-VARIABLE_ACCESS
						identifier='test'
					>
				b=
					<AST-EXPRESSION-INTEGER
						value='10'
					>
				compareType='LT'
			>
		block=
			<AST-BLOCK children=[
				<AST-STATEMENT-PRINT
					expression=
						<AST-EXPRESSION-INCREMENT
							identifier='test'
						>
				>
			>
	>,
	<AST-STATEMENT-IF
		condition=
			<AST-EXPRESSION-VARIABLE_ACCESS
				identifier='test'
			>
		block_if=
			<AST-BLOCK children=[
				<AST-STATEMENT-PRINT
					expression=
						<AST-EXPRESSION-INTEGER
							value='0'
						>
				>
			>
	>,
	<AST-STATEMENT-IF
		condition=
			<AST-EXPRESSION-VARIABLE_ACCESS
				identifier='test'
			>
		block_if=
			<AST-BLOCK children=[
				<AST-STATEMENT-PRINT
					expression=
						<AST-EXPRESSION-INTEGER
							value='1'
						>
				>
			>
		block_else=
			<AST-BLOCK children=[
				<AST-STATEMENT-PRINT
					expression=
						<AST-EXPRESSION-INTEGER
							value='2'
						>
				>
			>
	>
>